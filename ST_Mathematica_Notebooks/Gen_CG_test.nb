       " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"num", "<", "n"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"sb", " ", "=", " ", 
         RowBox[{"Sort", "[", "sb", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"ar", "=", 
         RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"counter", " ", "=", " ", "1"}], ";", " ", 
