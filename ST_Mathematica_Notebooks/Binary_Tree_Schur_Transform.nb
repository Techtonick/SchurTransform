Cell[BoxData[
 RowBox[{
  RowBox[{"TensorPermutation", "[", 
   RowBox[{"da_", ",", "db_"}], "]"}], ":=", 
  RowBox[{"SparseArray", "[", 
   RowBox[{"PermutationMatrix", "[", 
    RowBox[{"Join", "@@", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"j", "*", "db"}], "+", "i"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "db"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "0", ",", 
         RowBox[{"da", "-", "1"}]}], "}"}]}], "]"}]}], "]"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.896513175293913*^9, 3.8965131962937136`*^9}, 
   3.89651326003512*^9, {3.89651336628621*^9, 3.896513370167833*^9}, {
   3.8967856564727716`*^9, 3.8967856601683664`*^9}, {3.896802829566765*^9, 
