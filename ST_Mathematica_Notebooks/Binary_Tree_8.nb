 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Create", " ", "an", " ", "almost", " ", "transformed", " ", "TensMat", 
    " ", "for", " ", "8", " ", "qubits", " ", "and", " ", "turn", " ", "it", 
    " ", "into", " ", "block", " ", "diagonal", " ", "matrix"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"AlmostTransfTensMat8", "=", 
     RowBox[{"Simplify", "[", 
      RowBox[{
       RowBox[{"SparseArray", "[", "GCGSchur8", "]"}], ".", "TensMat", ".", 
       RowBox[{"SparseArray", "[", 
        RowBox[{"GCGSchur8", "\[Transpose]"}], "]"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"BlockMat", "=", 
     RowBox[{"BlockDiagonalMatrix", "[", "AlmostTransfTensMat8", "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Blocks", "=", 
     RowBox[{"BlockMat", "[", "\"\<Blocks\>\"", "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Sort", " ", "the", " ", "blocks", " ", "highest", " ", "dimension", " ", 
     "to", " ", "lowest", " ", "and", " ", "achieve", " ", "a", " ", 
     "transformed", " ", "TensMat"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"TransfTensMat8", "=", 
     RowBox[{"BlockDiagonalMatrix", "@", 
      RowBox[{"Reverse", "[", 
       RowBox[{"Blocks", "[", 
        RowBox[{"[", 
         RowBox[{"Ordering", "[", 
